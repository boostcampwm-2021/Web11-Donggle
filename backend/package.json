{
  "name": "backend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "./src/app.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node ./src/app.ts",
    "dev": "SET NODE_ENV=development && nodemon --delay 500ms -r tsconfig-paths/register ./src/app.ts",
    "prod": "SET NODE_ENV=production && pm2 start ./app.ts -i 0",
    "reload": "pm2 reload ./app.js"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "tsconfig-paths": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
