{
  "name": "backend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "./src/app.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development && nodemon --ignore test.json -r tsconfig-paths/register ./src/app.ts",
    "prod": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist && cross-env NODE_ENV=production && pm2 start ./dist/app.js -i 0",
    "reload": "pm2 reload ./dist/app.js"
  },
  "dependencies": {
    "@types/proj4": "^2.5.2",
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "pm2": "^5.1.2",
    "proj4": "^2.7.5",
    "simplify-js": "^1.2.4",
    "tsconfig-paths": "^3.11.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4"
  }
}
